#设置解压ramdisk压缩格式
sign_decompress_mode=gzip
#设置压缩ramdisk压缩格式
sign_compress_mode=gzip
#设置是否签名
sign_file=true
#设置签名模式
sign_mode=internal
#设置签名使用的文件 注：新版本只提供PlatformKey
sign_key=example
#设置固件开发代号
provider=example
#设置设备代号
device=example
#设置固件芯片架构
m_arch=example
#设置芯片指令集
m_cf=example
#设置物理内存
m_pmem=example
#设置网卡芯片
m_nc=example
#设置系统类型
m_os=example
#跳过备份标记true为跳过，其他值为备份
skip_backup=false
#文件版本标记
vendor_version=6.0
#全局APK签名开关
sign_file_globalapk=false
#Update.zip签名开关
sign_file_update=false
#Zipalign开关
global_zipalign_skip=false
#bootimg生成方式(使用makebootimg命令生成，则使用aosp挡否则使用nwaosp)
global_bootimg_mkbootimg=aosp
#设定系统工作模式  默认为zip\Normally (update-binaries/updater-script从Recovery刷机)
#如果设定参数无法正常运行时，系统将套用zip\Normally的这部分参数
#SYSMODE=zip\normally for Normally SmartPhoen and Pad
#SYSMODE=bin\AllwinnerB2B for Allwinner Binaries to Binaries
#SYSMODE=bin\AllwinnerB2z for Allwinner Binaries to Update.zip
#SYSMODE=bin\RK28xx for Rock-Chips RK2816\RK2818
#SYSMODE=bin\RK29xxV1 for Rock-Chips RK29 Cramfs
#SYSMODE=bin\RK29xxV2 for Rock-Chips RK29 Ext4FS
def_config_sysmode=zip\Normally
#KeyFile Password
password=your_password
#OUTMODE 输出模式(id为自动ID匹配,legency或留空为老式)
outmode=id
#定义id
id=ELCP-mPFFcf14r24dfafas45a7f
id_name=OUT
#清空模式
ro_format_system=erase